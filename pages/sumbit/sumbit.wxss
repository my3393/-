/* pages/sumbit/sumbit.wxss */

page{
  background: #f2f2f2;
  font-size: 30rpx;
}
.public{
 
  width: 100%;
  
  padding: 20rpx;
  background: #fff;
  box-sizing: border-box;
  
}
.fons{
  color: #000;
}
.enter{
  float: right;
  width: 16rpx;
  margin-right: 20rpx;
  height: 26rpx;
}
.dev{
  width: 90%;
  display: inline-block;
}
.enter image{
  width: 100%;
  height: 100%;
}
.texta{
  width: 100%;
  height: 300rpx;
}
.shang{
  color: #BBBBBB;
  color: 24rpx;
}
.dher{
  margin-top: 20rpx;
   width: 100%;
  overflow: scroll;
  white-space: nowrap; 
  -webkit-overflow-scrolling: touch;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
textarea{
  font-size: 26rpx;
}
.dher image{
  width: 200rpx;
  height: 200rpx;
  margin-right: 10rpx;
}
.imgs{
  display: inline-block;
}
.submit{
  width:calc((100% - 40rpx)/2);
  display: inline-block;
  margin-left: 20rpx;
  border-radius: 40rpx;
  height: 80rpx;
  margin-bottom: 30rpx;
  background: #F861AA;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
  margin-top: 40rpx;
}

/* 赛区选择 */

.ilebels{
    width: 100%;
    height: 600rpx;
    background-color: white;
    position: fixed;
    bottom: 0;
    left: 0;
}
.tops{
    display: flex;
    justify-content: space-between;
    padding: 0 20rpx;
    box-sizing: border-box;
    line-height: 80rpx;
}
.quxiao{
    color: #999999;
    font-size: 32rpx;
}
.sure{
    color: #1AAD19;
    font-size: 32rpx;
}
.left{
   width: 160rpx;
   background: #f2f2f2;
   height: 520rpx;
   overflow: hidden;
   overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
   float: left;
  
}

.right{
  float: right;
  width: calc(100% - 160rpx);
}
.clearfix:after {content: ""; display: block; height:0; clear:both; visibility: hidden;}
.left view{
  width: 160rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  color: #878787;
}
.sai-x{
  width: calc((100% - 50rpx)/3);
  margin-left: 10rpx;
  height: 64rpx;
  text-align: center;
  line-height: 64rpx;
  border: 1rpx solid #878787;
  color: #878787;
  border-radius: 8rpx;
  display: inline-block;
  margin-top: 20rpx;
}
.active{
  color: #fff;
  background: #F861AA;
  border: 0;
  border-radius: 6rpx;
}
.acti{
  background: #fff;
  
}
.der{
  position: relative;
  display: inline-block;
}
.detel{
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  top: 10rpx;
  right: 25rpx;
}
.detel image{
  width: 100%;
  height: 100%;
}

.container {
  border-top: 1px solid #f2f2f2;
}

.take {
  padding-left: 30rpx;
  line-height: 100rpx;
  height: 100rpx;
  background-color: #fff;
}

imgcut {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 90rpx;
  width: 100%;
}

.footer {
  line-height: 90rpx;
  position: fixed;
  bottom: 0px;
  left: 0;
  width: 100%;
  display: flex;
  justify-content: space-between;
  font-size: 40rpx;
}
.footer view{
  color: var(--color-primary);
  padding: 0 20rpx;
}

/* 海选告知书 */
.yds{
   width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.4);
    z-index: 12;
}
.yd{
	position:absolute;
  width: 600rpx;
  background: #fff;
  border-radius: 12rpx;
  text-align: center;
  font-size: 28rpx;
  padding-top: 40rpx;
	top: 40%;
	left: 50%;
	padding: 20rpx 20rpx;
	box-sizing: content-box;
  transform: translate(-50%,-50%)
}
.lip{
  margin: 20rpx 0;
  color: #A3A3A3;
  height: 600rpx;
  overflow: scroll;
}



.xioe{
  margin-top: 50rpx;
  line-height: 70rpx;
  display: flex;
  justify-content: space-around;
  color: #F861AA;
}
.xioe>view{
  width: 250rpx;
  height: 80rpx;
  border:2rpx solid #F861AA;
  border-radius: 60rpx;
  overflow: hidden;
  text-align: center;
  line-height: 80rpx;
  background: #F861AA;
  color: #fff;
}
.deter{
  border:2rpx solid #979797!important;
  background: #fff!important;
  color: #979797!important;
}
.cent{
  color: #000;
  text-align: center;
}

/* 第一步 */

.xinxi{
  padding: 20rpx;
  box-sizing: border-box;
  color: #A3A3A3;
}
.o-nav{
  padding: 20rpx;
  background: #fff;
  box-sizing: border-box;
  color: #000000;
}
.kuang{
  border: 1rpx solid #CCCCCC;
  height: 70rpx;
  border-radius: 10rpx;
  line-height: 70rpx;
  margin-top: 10rpx;
  margin: 20rpx 0;
}
.inpu{
  height: 70rpx;
  line-height: 70rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
  display: inline-block;
  width: 90%;
}
.xia{
  float: right;
  width: 36rpx;
  margin-right: 20rpx;
  height: 16rpx;
}
.xia image{
  width: 100%;
  height: 100%;
}

/* 地区选择 */
.mwrap{
	width: 100%;
	position: fixed;
	bottom: 0;
	left: 0;
}
.mtitle{

	height: 80rpx;
	line-height: 80rpx;
	background-color: #e6e6e6 !important;
	width: 100%;
	font-size: 30rpx;
	color: #333;
	text-align: center;
}
.province{
	background-color: white;
	width: 100%;
	height: 520rpx;
	overflow: scroll;
	-webkit-overflow-scrolling: touch;
}
.province view{
	line-height: 80rpx;
	padding-left: 40rpx;
	box-sizing: border-box;
	border-bottom: 1px solid #DDDDDD;
	font-size: 28rpx;
	color: #333;
}
.province_active{
	background-color: #FE5BA9;
	color: white !important;
}
.next{
   width: calc(100% - 40rpx);
  margin-left: 20rpx;
  border-radius: 40rpx;
  height: 80rpx;
  background: #F861AA;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
  margin-top: 40rpx;
}